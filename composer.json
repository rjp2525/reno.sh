{
    "name": "rjp2525/reno.sh",
    "type": "project",
    "description": "Personal portfolio website of Reno Philibert",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "filament/filament": "^4.0",
        "filament/spatie-laravel-tags-plugin": "^4.5",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.9",
        "laravel/framework": "^12.47",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.11",
        "laravel/wayfinder": "^0.1.13",
        "league/flysystem-aws-s3-v3": "^3.30",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "spatie/laravel-feed": "^4.4",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-tags": "^4.10",
        "tightenco/ziggy": "^2.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/breeze": "^2.3",
        "laravel/pint": "^1.27",
        "laravel/sail": "^1.52",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
